datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model AnomalyRule {
  condition   Json?
  createdAt   DateTime @default(now())
  description String?
  id          Int      @id @default(autoincrement())
  name        String?
  updatedAt   DateTime @updatedAt
}
